{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": [
      "types"
    ],
    "baseUrl": ".",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "paths": {
      "~/*": [
        "./*"
      ],
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
